<div id="sidebar" class="col-sm-4 col-md-3 col-lg-2 hidden-xs">
  <%-partial('_partial/avatar.ejs')%>
  <nav class="navbar ">
   <ul class="menu">
     <% for (name in theme.menu) { %>
       <li class="menu-item ">
         <a href="<%- url_for(theme.menu[name]) %>" class="menu-item-link btn btn-default btn-block"><%= name %></a>
       </li>
     <% } %>
   </ul>
   <button data-target="#category-nav" data-toggle="collapse" class="btn btn-primary  btn-block">category</button>
   <div id="category-nav" class="category-nav collapsing " >
     <%- list_categories({style:"",separator:""}) %>
   </div>
 </nav>
   <%-partial('_partial/tagcloud.ejs')%>
</div>
